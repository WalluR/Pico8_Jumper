function animate_start()
 start_animation = {
  time = 0,
  wait = 1
 }
 time()
 if time() - start_animation.time > start_animation.wait then
 swap_tile(18,1,10)
 swap_tile(19,1,11)
 swap_tile(18,2,26)
 swap_tile(18,2,27)
 start_animation.time = time()
end
if time() - start_animation.time > start_animation.wait then
swap_tile(18,1,8)
swap_tile(19,1,9)
swap_tile(18,2,24)
swap_tile(18,2,25)
start_animation.time = time()
end
end
