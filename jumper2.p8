-- our function that creates the moving tiles in the map and manages all the movements and deleting them.
 function self_obj()
  tile={
   x=15,
   y=flr(rnd(11)) + 2,
   sprite=17,
   time=0,
   wait=0.5,
   count=0,
   start_pos=15,
   swap = function(self)
    swap_tile(self.x,self.y,33)
   end,
   move = function(self)
    if self.start_pos < 0 then
     self.x=16
     self.start_pos=16
     self.time=0
     self.count=0
     self.y= flr(rnd(11)) + 2
    end
    self.x-=1
    self.count+=1
   end,
   swap_back = function(self)
    swap_tile(self.start_pos,self.y,32)
    self.start_pos-=1
   end,
   roll = function(self)
    time()
    if time() - self.time > self.wait then
     if (self.count > 3) then
     self:swap_back()
     end
    self:swap()
    self:move()
    self.time=time()
   end
  end,
  reset = function(self)
   for i=0,4 do
    if self.x + i == 16 then
     return false
    end
    swap_tile(self.x + i,self.y,32)
   end
  end
  }
  add(tiles,tile)
  end



