-- waiting function in cocreate thread.
function wait(t)
 for x=1,t do
  yield()
 end
end

--Using cocreate to send moving tiles to player. wait() is the time that how fast tiles move.
function start1()
local c = cocreate(function()
 for i=1,4 do
  move_tile(i)
  if i == 2 then
  end
  wait(100)
 end
end)
add(actions,c)
end

function start(game_time)
 options ={}
 options.time = 0
 options.wait = 1
 move_tile(1)
 if game_time - options.time > options.wait then
  move_tile(2)
 end
 if game_time - options.time > options.wait then

  move_tile(3)
 end
 if game_time - options.time > options.wait then

  move_tile(4)
 end
end


function move_tile(n)
 i=tiles[n]
 i:roll()
end


function swap_tile(x,y,numb)
 tile=mget(x,y)
 mset(x,y,numb)
end


